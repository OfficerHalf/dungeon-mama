{
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": true,
        "node": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "linebreak-style": "off",
        "indent": ["error", 4],
        "comma-dangle": "off"
    },
    "globals": {
        "pell": true,
        "Mark": true
    }
}